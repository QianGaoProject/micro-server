{
    "listeners": {
        "*:80": {
            "pass": "applications/fastapi"
        }
    },

    "applications": {
        "fastapi": {
            "type": "python 3.9",
            "path": "/usr/src/app/api",
            "home": "/usr/local",
            "module": "main",
            "callable": "app"
        }
    }
}